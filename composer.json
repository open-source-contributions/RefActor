{
    "name": "phpfui/refactor",
    "description": "A simple, fast and easy to use tool to refactor PHP code based on PhpParser",
    "minimum-stability": "stable",
    "keywords": ["refactor", "rector", "phpparser", "legacy", "parser", "PHP", "ast", "refactoring"],
    "homepage": "http://www.phpfui.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruce Wells",
            "email": "brucekwells@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "nikic/php-parser": "^4.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {"PHPFUI\\": "src/PHPFUI"}
    },
    "autoload-dev": {
        "psr-4": {"PHPFUI\\RefActor\\Tests\\": "tests"}
    }
}
